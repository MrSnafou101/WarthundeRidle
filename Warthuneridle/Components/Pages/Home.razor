@page "/"
@rendermode InteractiveServer
@inject JSONHandler JsonHandler

<PageTitle>Home</PageTitle>

<h1>Hello, world!</h1>

<button class="btn btn-primary" @onclick="TestJsonLoad">
    Click me
</button>

@code {
    private void TestJsonLoad()
    {
        var vehicles = JsonHandler.LoadGroundVehicleData();
        Console.WriteLine($"Loaded {vehicles.Count} vehicles");
        vehicles.ForEach(v => Console.WriteLine(v.VehicleName  + " " + v.Country.NationName));
    }
}
